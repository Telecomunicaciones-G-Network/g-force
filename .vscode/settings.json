{
  // Autoimports
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  // Formatting
  "editor.defaultFormatter": "biomejs.biome",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  // Hightlight Extension
  "todohighlight.isEnable": true,
  "todohighlight.isCaseSensitive": true,
  "todohighlight.keywords": [
    {
      "text": "DONE:",
      "color": "#ffffff",
      "backgroundColor": "#499167",
      "overviewRulerColor": "grey"
    },
    {
      "text": "IMPROVE:",
      "color": "#ffffff",
      "backgroundColor": "#2081c3",
      "overviewRulerColor": "grey"
    },
    {
      "text": "PENDING:",
      "color": "#ffffff",
      "backgroundColor": "#d84727",
      "overviewRulerColor": "grey"
    }
  ],
  // Material Icon theme
  "material-icon-theme.folders.associations": {
    "actions": "cue",
    "dictionaries": "resource",
    "entities": "svg",
    "mappers": "mappings",
    "presenters": "delta",
    "usecases": "vm",
    "viewmodels": "job"
  },
  "material-icon-theme.files.associations": {
    "*.action.ts": "cue",
    "*.class.ts": "capnp",
    "*.constant.ts": "command",
    "*.dependency.ts": "varnish",
    "*.dictionary.ts": "denizenscript",
    "*.dto.ts": "hurl",
    "*.entity.ts": "sketch",
    "*.error.ts": "erlang",
    "*.exception.ts": "ember",
    "*.fetcher.ts": "salesforce",
    "*.interface.ts": "less",
    "*.mapper.ts": "xml",
    "*.presenter.ts": "sentry",
    "*.repository.ts": "wxt",
    "*.util.ts": "credits",
    "css-data.json": "vscode",
    "settings.json": "vscode"
  },
  "material-icon-theme.folders.customClones": [
    {
      "name": "Adapters",
      "base": "connection",
      "color": "red-500",
      "folderNames": ["adapters"]
    },
    {
      "name": "Application",
      "base": "src",
      "color": "blue-500",
      "folderNames": ["app", "application"]
    },
    {
      "name": "Domain",
      "base": "src",
      "color": "yellow-800",
      "folderNames": ["domain"]
    },
    {
      "name": "DTOs",
      "base": "connection",
      "color": "pink-400",
      "folderNames": ["dtos"]
    },
    {
      "name": "Exceptions",
      "base": "error",
      "color": "orange-500",
      "folderNames": ["exceptions"]
    },
    {
      "name": "Fetchers",
      "base": "aws",
      "color": "light-blue-500",
      "folderNames": ["fetchers"]
    },
    {
      "name": "HTTP Client",
      "base": "public",
      "color": "green-500",
      "folderNames": ["http-client"]
    },
    {
      "name": "Hooks",
      "base": "public",
      "color": "green-500",
      "folderNames": ["hooks"]
    },
    {
      "name": "Infrastructure",
      "base": "src",
      "color": "green-500",
      "folderNames": ["infrastructure"]
    },
    {
      "name": "Modules",
      "base": "plugin",
      "color": "red-500",
      "folderNames": ["modules"]
    },
    {
      "name": "Packages",
      "base": "packages",
      "color": "green-600",
      "folderNames": ["packages"]
    },
    {
      "name": "Queries",
      "base": "command",
      "color": "deep-orange-400",
      "folderNames": ["queries"]
    },
    {
      "name": "Stringify",
      "base": "font",
      "color": "amber-700",
      "folderNames": ["stringify"]
    },
    {
      "name": "User",
      "base": "admin",
      "color": "blue-600",
      "folderNames": ["user"]
    }
  ],
  "material-icon-theme.files.customClones": [
    {
      "name": "Adapters",
      "base": "capacitor",
      "color": "red-400",
      "fileExtensions": ["adapter.ts"]
    },
    {
      "name": "Commands",
      "base": "templ",
      "color": "blue-500",
      "fileExtensions": ["command.ts"]
    },
    {
      "name": "Enums",
      "base": "ember",
      "color": "teal-400",
      "fileExtensions": ["enum.ts"]
    },
    {
      "name": "Queries",
      "base": "templ",
      "color": "deep-orange-400",
      "fileExtensions": ["query.ts"]
    },
    {
      "name": "React Hooks",
      "base": "react",
      "color": "amber-300",
      "fileExtensions": ["hook.tsx"]
    },
    {
      "name": "Types",
      "base": "typst",
      "color": "blue-500",
      "fileExtensions": ["type.ts"]
    },
    {
      "name": "Usecase Get",
      "base": "powershell",
      "color": "green-500",
      "fileNames": ["get-contacts.usecase.ts"]
    },
    {
      "name": "Usecase Post",
      "base": "powershell",
      "color": "yellow-700",
      "fileNames": ["login.usecase.ts"]
    },
    {
      "name": "View Models",
      "base": "vagrant",
      "color": "yellow-700",
      "fileExtensions": ["viewmodel.ts"]
    }
  ],
  // Peacok Configuration
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#1a1a1a",
    "activityBar.background": "#1a1a1a",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#606020",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#1a1a1a",
    "statusBar.background": "#000000",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#1a1a1a",
    "statusBarItem.remoteBackground": "#000000",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#00000099",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "000000",
  // TailwindCSS
  "css.customData": [".vscode/css-data.json"],
  // Terminal integrated
  "terminal.integrated.fontFamily": "'Monaspace Radon', 'Victor Mono', Consolas, 'Courier New', monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "600",
  // TypeScript
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  // Vscode Editor
  "workbench.activityBar.location": "hidden",
  "editor.codeActionsOnSave": {
    "source.fixAll.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "editor.fontFamily": "'Monaspace Radon', 'Victor Mono', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": "600",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  // Vscode Features
  "editor.stickyTabStops": true,
  "editor.stickyScroll.enabled": true,
  "workbench.statusBar.visible": true,
  "workbench.editor.showTabs": "multiple",
  "workbench.sideBar.location": "right",
  "workbench.tree.indent": 8,
  "workbench.tree.renderIndentGuides": "always",
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  }
}
